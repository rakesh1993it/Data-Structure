   "source": [
    "# Bubble Sort"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 25,
   "metadata": {
    "collapsed": true
   },
   "outputs": [],
   "source": [
    "def sort(seq):\n",
    "    L = len(seq)\n",
    "    for i in range(L):\n",
    "        for j in range(0, L-i-1):\n",
    "            if(seq[j] > seq[j+1]):\n",
    "                seq[j], seq[j+1] = seq[j+1], seq[j]\n",
    "    return seq            "
   ]
  },
