   "source": [
    "# Binary Search"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {
    "collapsed": true
   },
   "outputs": [],
   "source": [
    "def search(seq, key):\n",
    "    low = 0\n",
    "    heigh = len(seq)-1\n",
    "    \n",
    "    while(heigh >= low):\n",
    "        mid = low+(heigh-low)//2\n",
    "        \n",
    "        if(seq[mid]<key):\n",
    "            low = mid+1\n",
    "        elif(seq[mid]>key):\n",
    "            heigh = mid-1\n",
    "        else:\n",
    "            return(mid)\n",
    "        \n",
    "    return False"
   ]
  },
