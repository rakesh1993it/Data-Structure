   "source": [
    "def Linear_Search(seq,key):\n",
    "    height = len(seq) - 1\n",
    "    for i in range(height):\n",
    "        if(seq[i] == key):\n",
    "            return(i)\n",
    "        \n",
    "    return False"
   ]
